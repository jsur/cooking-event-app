mixin eventCard(event = {})
  .eventcard
    .event-card-image
      img(src=event.image.path)
    .event-card.info
      p Name: #{event.title}
      p Food type: #{event.foodtype}
      p Price: #{event.price} â‚¬
      a(href='/event/'+event._id) Event details
      p Places left: #{event.placesLeft}
